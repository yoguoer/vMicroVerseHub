{
  "name": "vmicroversehub",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "vivien",
  "license": "ISC",
  "description": "vMicroversehub 是一个使用 v-micro-app-plugin（基于京东推出的 MicroApp，技术栈无关，开箱即用） 搭建的 monorepo 微前端项目框架。",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "clean": "pnpx rimraf node_modules",
    "clean:cache": "pnpx rimraf node_modules/.cache",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "format": "prettier --write src/",
    "ts:check": "pnpm vue-tsc --noEmit --skipLibCheck",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky",
    "commit": "git add -A && czg && git push"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@micro-zoe/micro-app": "1.0.0-rc.6",
    "element-plus": "^2.7.7",
    "less": "^4.2.0",
    "path": "^0.12.7",
    "pinia": "^2.2.1",
    "v-micro-app-plugin": "^1.0.67",
    "vue": "^3.4.35",
    "vue-router": "^4.4.0"
  },
  "keywords": [
    "v-micro-app-plugin",
    "vite",
    "vue",
    "micro-frontend",
    "microApp"
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.14.15",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-legacy": "^5.3.1",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "czg": "^1.11.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "js-cookie": "^3.0.5",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.9.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.4.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^7.3.7",
    "vue-tsc": "^2.0.29"
  }
}
